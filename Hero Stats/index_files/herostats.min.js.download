oes[t].AGIGain;var w=heroes[t].AGI;var E=w+mathlevel*d+2*stats;$(this).text(Math.floor(E))}else if(u=="BaseINT"){var c=heroes[t].INTGain;var S=heroes[t].INT;var x=S+mathlevel*c+2*stats;$(this).text(Math.floor(x))}else if(u=="BasePrimaryStat"){if(g=="STR"){var f=heroes[t].STRGain;var y=heroes[t].STR;var b=y+mathlevel*f+2*stats;$(this).text(Math.floor(b))}else if(g=="AGI"){var d=heroes[t].AGIGain;var w=heroes[t].AGI;var E=w+mathlevel*d+2*stats;$(this).text(Math.floor(E))}else{var c=heroes[t].INTGain;var S=heroes[t].INT;var x=S+mathlevel*c+2*stats;$(this).text(Math.floor(x))}}else if(u=="HPRegen"){var f=heroes[t].STRGain;var y=heroes[t].STR;var T=heroes[t].HPRegen;var N=T+.03*y+.03*mathlevel*f+.03*stats;$(this).text(N.toFixed(1))}else if(u=="ManaRegen"){var c=heroes[t].INTGain;var S=heroes[t].INT;var T=heroes[t].ManaRegen;var C=T+.04*S+.04*mathlevel*c+.04*stats;$(this).text(C.toFixed(1))}else if(u=="AvgBase"){var f=heroes[t].STRGain;var y=heroes[t].STR;var b=y+mathlevel*f+2*stats;var d=heroes[t].AGIGain;var w=heroes[t].AGI;var E=w+mathlevel*d+2*stats;var c=heroes[t].INTGain;var S=heroes[t].INT;var x=S+mathlevel*c+2*stats;var k=(x+E+b)/3;$(this).text(Math.floor(k))}else if(u=="APS"){var d=heroes[t].AGIGain;var w=heroes[t].AGI;var E=w+mathlevel*d+2*stats;var L=heroes[t].BAT;var A=(100+E)/100;var O=A/L;$(this).text(O.toFixed(3))}else if(u=="Legs"){var M=heroes[t].Hero;if(M=="Meepo"){if(level>=18){$(this).text(8)}else if(level>=11){$(this).text(6)}else if(level>=4){$(this).text(4)}}else if(M=="Anti-Mage"){if(level>=16){$(this).text(1.9)}else if(level>=11){$(this).text(1.8)}else if(level>=6){$(this).text(1.5)}}else{$(this).text(heroes[t].Legs)}}stats=o})})}function sendReport(){var e=$("#type").val();var t=$("#desc").val();if(t==""){$("#desc").css("background-color","rgb(207, 131, 130)");$("#desc").select()}else{$("#desc").val("");$("#desc").css("background-color","#FFFFFF");$("#report").modal("hide");$.post("report.php",{type:e,desc:t},function(e){alert(e)})}}function filt